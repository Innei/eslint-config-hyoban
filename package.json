{
	"name": "eslint-config-hyoban",
	"version": "0.7.7",
	"description": "Hyoban's ESLint Config",
	"keywords": [],
	"homepage": "https://github.com/hyoban/eslint-config-hyoban#readme",
	"bugs": "https://github.com/hyoban/eslint-config-hyoban/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hyoban/eslint-config-hyoban.git"
	},
	"license": "MIT",
	"author": "Stephen Zhou <hi@hyoban.cc>",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"bin": {
		"eslint-config-hyoban": "./dist/bin/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "bunchee",
		"build:inspector": "npx @eslint/config-inspector@latest build",
		"dev": "bunchee -w",
		"lint": "prettier --check . && eslint .",
		"lint:fix": "prettier --write . && eslint . --fix",
		"prepare": "pnpm run build",
		"prepublishOnly": "pnpm run build",
		"start": "tsx src/index.ts",
		"test": "vitest",
		"typecheck": "tsc"
	},
	"dependencies": {
		"@eslint-react/eslint-plugin": "^1.5.8",
		"@eslint/js": "^9.0.0",
		"@types/eslint": "^8.56.8",
		"defu": "^6.1.4",
		"eslint-config-flat-gitignore": "^0.1.5",
		"eslint-plugin-antfu": "^2.1.2",
		"eslint-plugin-hyoban": "^0.2.7",
		"eslint-plugin-import-x": "^0.5.0",
		"eslint-plugin-package-json": "^0.12.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^12.0.0",
		"eslint-plugin-unicorn": "^52.0.0",
		"jsonc-eslint-parser": "^2.4.0",
		"typescript-eslint": "^7.6.0"
	},
	"devDependencies": {
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "20.11.30",
		"bunchee": "^5.1.2",
		"eslint": "^9.0.0",
		"eslint-typegen": "^0.2.2",
		"prettier": "^3.2.5",
		"tsx": "^4.7.2",
		"typescript": "^5.4.5",
		"vitest": "^1.4.0"
	},
	"peerDependencies": {
		"eslint": "^8.56.0 || ^9.0.0",
		"typescript": ">=4.7.4"
	},
	"packageManager": "pnpm@8.15.6"
}
